<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Cidade.
    -->
    <changeSet id="20240120164817-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="estado_id"
                                 baseTableName="cidade"
                                 constraintName="fk_cidade__estado_id"
                                 referencedColumnNames="id"
                                 referencedTableName="estado"
                                 />

        <addForeignKeyConstraint baseColumnNames="embarcador_id"
                                 baseTableName="cidade"
                                 constraintName="fk_cidade__embarcador_id"
                                 referencedColumnNames="id"
                                 referencedTableName="embarcador"
                                 />

        <addForeignKeyConstraint baseColumnNames="transportadora_id"
                                 baseTableName="cidade"
                                 constraintName="fk_cidade__transportadora_id"
                                 referencedColumnNames="id"
                                 referencedTableName="transportadora"
                                 />
    </changeSet>
</databaseChangeLog>
